{
  "name": "prouter",
  "description": "The power of the express's routing-expressions style in the frontend",
  "version": "3.1.20",
  "main": "prouter.min.js",
  "homepage": "https://github.com/rogerpadilla/prouter",
  "bugs": {
    "url": "https://github.com/rogerpadilla/prouter/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rogerpadilla/prouter.git"
  },
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "rimraf coverage && npm run lint && jest",
    "jest": "jest",
    "start": "rimraf dist && webpack --progress --profile --bail --watch",
    "build": "rimraf dist && webpack --progress --profile --bail --env=prod"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "transform": {
      "^.+\\.ts$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/*.spec.ts"
    ],
    "mapCoverage": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageReporters": [
      "html",
      "lcov"
    ],
    "coverageDirectory": "coverage",
    "testURL": "http://example.com"
  },
  "keywords": [
    "browser",
    "router"
  ],
  "license": "MIT",
  "contributors": [
    "Roger Padilla <rogerpadilla>"
  ],
  "dependencies": {
    "path-to-regexp": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "21.1.2",
    "@types/node": "8.0.33",
    "awesome-typescript-loader": "3.2.3",
    "copy-webpack-plugin": "4.1.1",
    "jest": "21.2.1",
    "phantomjs-prebuilt": "2.1.15",
    "rimraf": "2.6.2",
    "source-map-loader": "0.2.2",
    "tslint": "5.7.0",
    "typescript": "2.5.3",
    "webpack": "3.6.0"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
