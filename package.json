{
  "name": "prouter",
  "description": "The power of the express's routing-expressions style in the frontend",
  "version": "6.0.0",
  "main": "prouter.min.js",
  "homepage": "https://github.com/rogerpadilla/prouter",
  "bugs": {
    "url": "https://github.com/rogerpadilla/prouter/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rogerpadilla/prouter.git"
  },
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "rimraf coverage && npm run lint && jest",
    "jest": "jest",
    "jest.watch": "jest --watch",
    "jest.watchAll": "jest --watchAll",
    "start": "rimraf dist && webpack --progress --profile --bail --watch --mode=development",
    "build": "rimraf dist && webpack --progress --profile --bail --mode=production"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.ts"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/*.d.ts"
    ],
    "coverageReporters": [
      "html",
      "lcov"
    ],
    "coverageDirectory": "coverage",
    "testURL": "http://example.com",
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    }
  },
  "keywords": [
    "browser",
    "router"
  ],
  "license": "MIT",
  "contributors": [
    "Roger Padilla <rogerpadilla>"
  ],
  "dependencies": {
    "path-to-regexp": "^2.2.1"
  },
  "devDependencies": {
    "@types/jest": "23.3.1",
    "@types/node": "10.7.0",
    "awesome-typescript-loader": "5.2.0",
    "copy-webpack-plugin": "4.5.2",
    "jest": "23.5.0",
    "rimraf": "2.6.2",
    "source-map-loader": "0.2.4",
    "ts-jest": "23.0.1",
    "tslint": "5.11.0",
    "typescript": "3.0.1",
    "webpack": "4.16.5",
    "webpack-cli": "3.1.0"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
