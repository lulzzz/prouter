{
  "name": "prouter",
  "description": "The power of the express's routing-expressions style in the frontend",
  "version": "4.0.5",
  "main": "prouter.min.js",
  "homepage": "https://github.com/rogerpadilla/prouter",
  "bugs": {
    "url": "https://github.com/rogerpadilla/prouter/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rogerpadilla/prouter.git"
  },
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "rimraf coverage && npm run lint && jest",
    "jest": "jest",
    "start": "rimraf dist && webpack --progress --profile --bail --watch",
    "build": "rimraf dist && webpack --progress --profile --bail --env=prod"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.ts"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/*.d.ts"
    ],
    "coverageReporters": [
      "html"
    ],
    "mapCoverage": true,
    "coverageDirectory": "coverage",
    "testURL": "http://example.com",
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    }
  },
  "keywords": [
    "browser",
    "router"
  ],
  "license": "MIT",
  "contributors": [
    "Roger Padilla <rogerpadilla>"
  ],
  "dependencies": {
    "path-to-regexp": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "21.1.6",
    "@types/node": "8.0.53",
    "awesome-typescript-loader": "3.4.0",
    "copy-webpack-plugin": "4.2.1",
    "jest": "21.2.1",
    "rimraf": "2.6.2",
    "source-map-loader": "0.2.3",
    "ts-jest": "21.2.3",
    "tslint": "5.8.0",
    "typescript": "2.6.1",
    "webpack": "3.8.1"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
